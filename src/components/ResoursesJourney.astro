---

<div class="self-start mt-12 ml-24">
  <h4 class="font-medium text-base">Resources</h4>
  <ul class="pt-4 flex flex-col gap-2">
    {
      resoursesLinks.map((el) => (
        <li>
          <a
            href={el.link}
            class="text-base text-gray-400 flex items-center hover:text-foreground hover:underline whitespace-nowrap"
            target="_blank"
          >
            {el.nameLink}
          </a>
        </li>
      ))
    }
  </ul>
</div>
